{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "rewriteRelativeImportExtensions": true,
        "allowJs": true,
        "checkJs": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": false,
        "noImplicitAny": false,
        "moduleResolution": "bundler"
    }
}